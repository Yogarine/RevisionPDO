{
  "name": "yogarine/revision-pdo",
  "description": "RevisionPDO",
  "type": "library",
  "homepage": "https://github.com/Yogarine/RevisionPDO",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Alwin Garside",
      "email": "alwin@garsi.de"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-pdo": "*",
    "greenlion/php-sql-parser": "^4.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0.0,<8.0.0"
  },
  "suggest": {
    "yogarine/revision-pdo-laravel": "RevisionPDO integration for Laravel"
  },
  "autoload" : {
    "psr-0" : {
      "RevisionPDO\\" : "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
